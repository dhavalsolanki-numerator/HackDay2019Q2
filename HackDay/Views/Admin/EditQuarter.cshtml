                    <ul class="header-dropdown m-r--5">
                        <a href="/admin/quarters" class="btn bg-green waves-effect">BACK</a>
                    </ul>
                </div>
                <div class="body">
                    <form id="form_validation">

                        <label for="name">Name</label>
                        <div class="form-group">
                            <div class="form-line">
                                <input type="text" id="name" class="form-control" placeholder="Enter Name" required>
                            </div>
                        </div>

                        <div class="row clearfix">

                            <div class="col-xs-4">
                                <h2 class="card-inside-title">Event Start Date</h2>
                                <div class="input-group date" id="bs_datepicker_component_container">
                                    <div class="form-line">
                                        <input type="text" class="form-control" placeholder="Choose start date...">
                                    </div>
                                    <span class="input-group-addon">
                                        <i class="material-icons">date_range</i>
                                    </span>
                                </div>
                            </div>
                            <div class="col-xs-4">
                                <h2 class="card-inside-title">Event End Date</h2>
                                <div class="input-group date" id="bs_datepicker_component_container1">
                                    <div class="form-line">
                                        <input type="text" class="form-control" placeholder="Choose end date...">
                                    </div>
                                    <span class="input-group-addon">
                                        <i class="material-icons">date_range</i>
                                    </span>
                                </div>
                            </div>
                        </div>

                        <div class="row clearfix">
                            <div class="col-xs-4">
                                <h2 class="card-inside-title">Registration Start Date</h2>
                                <div class="input-group date" id="bs_datepicker_component_container">
                                    <div class="form-line">
                                        <input type="text" class="form-control" placeholder="Choose start date...">
                                    </div>
                                    <span class="input-group-addon">
                                        <i class="material-icons">date_range</i>
                                    </span>
                                </div>
                            </div>
                            <div class="col-xs-4">
                                <h2 class="card-inside-title">Registration End Date</h2>
                                <div class="input-group date" id="bs_datepicker_component_container1">
                                    <div class="form-line">
                                        <input type="text" class="form-control" placeholder="Choose end date...">
                                    </div>
                                    <span class="input-group-addon">
                                        <i class="material-icons">date_range</i>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <!-- TinyMCE -->
                        <div class="row clearfix">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
